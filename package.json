{
  "name": "youtubetv",
  "version": "0.0.1",
  "description": "Youtube TV Wrapper for SteamOS",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "dist": "electron-builder"
  },
  "build": {
    "appId": "com.thegamershollow.youtubetv",
    "linux": {
      "target": [
        "AppImage"
      ],
      "category": "Utility",
      "artifactName": "youtubetv.${ext}"
    }
  },
  "repository": "https://github.com/thegamershollow/youtube-electron-app",
  "keywords": [
    "youtube",
    "tv",
    "wrapper",
    "steamos"
  ],
  "author": "TheGamersHollow",
  "license": "MIT",
  "devDependencies": {
    "electron": "^35.0.1",
    "electron-builder": "^25.1.8"
  },
  "dependencies": {
    "@ghostery/adblocker-electron": "^2.5.0",
    "cross-fetch": "^4.1.0"
  }
}
